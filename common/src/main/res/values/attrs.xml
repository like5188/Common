<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RotateTextView">
        <attr name="degree" format="float" />
    </declare-styleable>

    <declare-styleable name="AspectRatioImageView">
        <attr name="width_ratio" format="float" />
        <attr name="height_ratio" format="float" />
    </declare-styleable>

    <declare-styleable name="Titlebar">
        <attr name="center_gravity">
            <flag name="left" value="0" />
            <flag name="center_horizontal" value="1" />
        </attr>
    </declare-styleable>

    <!--TimerTextView显示文本-->
    <declare-styleable name="TimerTextView">
        <attr name="onStartText" format="string" />
        <attr name="onTickPrefixText" format="string" />
        <attr name="onTickSuffixText" format="string" />
        <attr name="onEndText" format="string" />
    </declare-styleable>
</resources>